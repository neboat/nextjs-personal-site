    "debugToolBar.background": "#141414",
    "titleBar.activeBackground": "#222222",
    "titleBar.activeForeground": "#E6E6E6",
    "titleBar.inactiveBackground": "#222222",
    "titleBar.inactiveForeground": "#7A7A7A",
    "focusBorder": "#0077B5",
