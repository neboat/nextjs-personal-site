      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": ["storage.type.built-in", "storage.type.return-type.lambda", "entity.name.type.defined", "entity.name.type.parameter"],
      "settings": {
        "foreground": "#689300"
      }
    },
    {
      "scope": ["storage.type.struct", "storage.type.class", "storage.type.namespace", "storage.type.template"],
      "settings": {
        "foreground": "#9900F8"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#9900F8"
      }
    },
    {
      "scope": ["storage.modifier.pointer", "storage.modifier.reference", "storage.modifier.array"],
      "settings": {
        "foreground": "#632618"
      }
    },
    {
      "scope": ["entity.name.namespace", "entity.name.scope-resolution"],
      "settings": {
        "foreground": "#33CCCC"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#BB4444"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#d16969"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": [
        "support.type.vendored.property-name",
        "support.type.property-name",
        "variable.css",
        "variable.scss",
        "variable.other.less",
        "source.coffee.embedded"
      ],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#9900F8"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#569cd6"
      }
